    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ store_manager.id }}</td>
            </tr>
            <tr>
                <th>NIC</th>
                <td>{{ store_manager.NIC }}</td>
            </tr>
