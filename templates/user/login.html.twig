<meta name="viewport" content="width=device-width, initial-scale=1">
<meta name="description" content="">
<meta name="author" content="">
<!-- Favicon icon -->
<link rel="icon" type="image/png" sizes="16x16" href="{{ asset('back/images/favicon.png') }}">
<title>Login</title>
<!-- Bootstrap Core CSS -->
<link href="{{ asset('back/css/bootstrap4.2.1.min.css') }}" rel="stylesheet">
<!-- page css -->
<link href="{{ asset('back/css/login-register-lock.css') }}" rel="stylesheet">
<!-- Custom CSS -->
<link href="{{ asset('back/css/master-stylesheet.css') }}" rel="stylesheet">

<!-- You can change the theme colors from here -->
<link href="{{ asset('back/css/style2.css') }}"   rel="stylesheet">
<!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
