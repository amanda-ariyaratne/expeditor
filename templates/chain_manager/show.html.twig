{% extends 'base.html.twig' %}

{% block title %}ChainManager{% endblock %}

{% block body %}
    <h1>ChainManager</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ chain_manager.id }}</td>
            </tr>
            <tr>
                <th>NIC</th>
                <td>{{ chain_manager.NIC }}</td>
            </tr>
            <tr>
                <th>Service_no</th>
                <td>{{ chain_manager.serviceNo }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ chain_manager.createdAt ? chain_manager.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Updated_at</th>
                <td>{{ chain_manager.updatedAt ? chain_manager.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Deleted_at</th>
                <td>{{ chain_manager.deletedAt ? chain_manager.deletedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('chain_manager_index') }}">back to list</a>

    <a href="{{ path('chain_manager_edit', {'id': chain_manager.id}) }}">edit</a>

    {{ include('chain_manager/_delete_form.html.twig') }}
